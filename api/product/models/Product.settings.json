{
  "connection": "default",
  "collectionName": "",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "price": {
      "type": "integer"
    },
    "stock": {
      "type": "integer"
    },
    "createdAt": {
      "type": "date"
    },
    "updatedAt": {
      "type": "date"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "user": {
      "model": "user",
      "via": "products",
      "plugin": "users-permissions"
    },
    "category": {
      "model": "category",
      "via": "products"
    },
    "order": {
      "model": "order",
      "via": "product"
    }
  }
}
